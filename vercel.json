{
    "builds": [{
        "src": "djangoProject/wsgi.py",
        "use": "@vercel/python",
        "config": { 
            "maxLambdaSize": "15mb", 
            "runtime": "python3.9",
            "maxDuration": 900  // Увеличиваем время выполнения до 15 минут (900 секунд)
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "djangoProject/wsgi.py"
        }
    ]
}
